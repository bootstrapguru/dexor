{
    "name": "bootstrapguru/dexor",
    "description": "An AI Engineer that actually codes for you",
    "keywords": ["ai", "dexor", "developer", "console", "cli"],
    "homepage": "https://dexor.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/bootstrapguru/dexor/issues",
        "source": "https://github.com/bootstrapguru/dexor"
    },
    "authors": [
        {
            "name": "Vijay Tupakula",
            "email": "hello@vijaykumar.me"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/psr7": "^2.6",
        "illuminate/database": "^11.5",
        "illuminate/routing": "^11.18",
        "illuminate/view": "^11.5",
        "laravel-zero/phar-updater": "^1.4",
        "laravel/dusk": "^8.2",
        "openai-php/client": "^0.10.1",
        "saloonphp/saloon": "^3.9",
        "spatie/laravel-data": "^4.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-zero/framework": "^11.0.0",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/dexor"]
}
